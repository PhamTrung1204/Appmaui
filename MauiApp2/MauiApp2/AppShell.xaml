<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:MauiApp2.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiApp2">

    
    
    <ShellContent Title="Newest Questions" ContentTemplate="{DataTemplate pages:HomePage}" Route="HomePage"/>
    <ShellContent Title="Followed Questions" ContentTemplate="{DataTemplate pages:FollowQuestionsPage}" Route="FollowQuestionsPage"/>
    <ShellContent Title="My Questions" ContentTemplate="{DataTemplate pages:MyQuestionsPage}" Route="MyQuestionsPage"/>
    <ShellContent Title="Active Members" ContentTemplate="{DataTemplate pages:ActiveMembersPage}" Route="ActiveMembersPage"/>

</Shell>
